#! /usr/local/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
:vpi_module "/usr/local/lib/ivl/v2009.vpi";
S_0x1cc84a0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x1cc8630 .scope module, "tb_counter" "tb_counter" 3 5;
 .timescale -9 -12;
v0x1cd9390_0 .var "clk", 0 0;
v0x1cd9460_0 .net "count", 3 0, v0x1cd9160_0;  1 drivers
v0x1cd9530_0 .var "rst", 0 0;
S_0x1ca3580 .scope module, "uut" "counter" 3 10, 4 1 0, S_0x1cc8630;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "count";
v0x1ca37d0_0 .net "clk", 0 0, v0x1cd9390_0;  1 drivers
v0x1cd9160_0 .var "count", 3 0;
v0x1cd9240_0 .net "rst", 0 0, v0x1cd9530_0;  1 drivers
E_0x1cc9410 .event posedge, v0x1cd9240_0, v0x1ca37d0_0;
    .scope S_0x1ca3580;
T_0 ;
    %wait E_0x1cc9410;
    %load/vec4 v0x1cd9240_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x1cd9160_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x1cd9160_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x1cd9160_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1cc8630;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1cd9390_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x1cc8630;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x1cd9390_0;
    %inv;
    %store/vec4 v0x1cd9390_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x1cc8630;
T_3 ;
    %vpi_call/w 3 21 "$dumpfile", "counter.vcd" {0 0 0};
    %vpi_call/w 3 22 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x1cc8630 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1cd9530_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1cd9530_0, 0, 1;
    %delay 100000, 0;
    %vpi_call/w 3 29 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "-";
    "testbench.sv";
    "design.sv";
